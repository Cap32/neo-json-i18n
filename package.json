{
  "name": "translate-json-values",
  "description": "translate json values over google translate",
  "version": "0.0.0",
  "main": "lib/translateJsonValues",
  "bin": {
    "translate-json-values": "bin/translate-json-values"
  },
  "files": [
    "*.md",
    "bin",
    "lib"
  ],
  "scripts": {
    "start": "yarn test --watchAll",
    "test": "cross-env NODE_ENV=development jest",
    "cli": "cross-env NODE_ENV=development node ./bin/translate-json-values",
    "prettier": "prettier-eslint --write \"+(src|test)/**/*.js\"",
    "prebuild": "rimraf lib",
    "build": "babel src -d lib",
    "preversion": "yarn test && yarn build"
  },
  "author": "Cap32",
  "repository": "Cap32/translate-json-values",
  "engines": {
    "node": ">=7.6"
  },
  "keywords": [],
  "license": "MIT",
  "dependencies": {
    "ajv": "^6.2.1",
    "fs-extra": "^5.0.0",
    "google-translate-api": "^2.3.0",
    "mkdirp": "^0.5.1",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.13.2",
    "babel-jest": "^22.4.1",
    "babel-plugin-transform-es2015-destructuring": "^6.19.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-plugin-transform-es2015-parameters": "^6.18.0",
    "babel-plugin-transform-es2015-spread": "^6.22.0",
    "babel-polyfill": "^6.13.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-0-without-async": "^1.0.0",
    "babel-register": "^6.11.6",
    "cross-env": "^4.0.0",
    "eslint": "^4.18.1",
    "eslint-config-cantonjs": "^0.2.0",
    "jest": "^22.4.2",
    "prettier-eslint-cli": "^4.7.1",
    "rimraf": "^2.5.4"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/jsonI18n.js"
    ],
    "modulePathIgnorePatterns": [
      "node_modules",
      "lib"
    ]
  }
}
